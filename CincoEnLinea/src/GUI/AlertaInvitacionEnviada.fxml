<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="200.0" prefWidth="340.0" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="GUI.AlertaInvitacionEnviadaController">
   <children>
      <ImageView fitHeight="118.0" fitWidth="119.0" layoutX="208.0" layoutY="74.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../resources/2000px-TK_email_icon.svg.png" />
         </image>
      </ImageView>
      <Label fx:id="invitacionEnviada" layoutX="21.0" layoutY="35.0" text="%invitacionEnviada">
         <font>
            <Font name="System Bold" size="22.0" />
         </font>
      </Label>
      <Label fx:id="exitosamente" layoutX="21.0" layoutY="67.0" prefHeight="32.0" prefWidth="169.0" text="%exitosamente">
         <font>
            <Font name="System Bold" size="22.0" />
         </font>
      </Label>
      <JFXButton fx:id="botonAceptar" buttonType="RAISED" layoutX="21.0" layoutY="150.0" onAction="#clicAceptar" prefHeight="25.0" prefWidth="80.0" style="-fx-background-color: #dbddff;" text="%aceptar" />
   </children>
</AnchorPane>
