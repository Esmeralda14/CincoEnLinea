            <Font name="Malayalam Sangam MN" size="16.0" />
         </font>
      </Button>
      <Button fx:id="registrar" layoutX="156.0" layoutY="471.0" mnemonicParsing="false" onAction="#abrirRegistrar" prefHeight="37.0" prefWidth="142.0" text="%registrar">
         <font>
            <Font name="Malayalam Sangam MN" size="16.0" />
         </font>
      </Button>
      <PasswordField fx:id="fieldContraseña" layoutX="102.0" layoutY="367.0" prefHeight="27.0" prefWidth="250.0">
         <effect>
            <ColorAdjust contrast="-0.2" />
         </effect></PasswordField>
      <ImageView fitHeight="136.0" fitWidth="173.0" layoutX="158.0" layoutY="97.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../resources/photo_2017-09-09_20-11-53.jpg" />
         </image>
         <effect>
            <ColorAdjust contrast="1.0" />
         </effect>
      </ImageView>
      <JFXButton fx:id="espanol" layoutX="290.0" layoutY="58.0" onAction="#cambiarIdiomaEs" text="%espanol" underline="true">
