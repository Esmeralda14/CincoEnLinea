            <RadialGradient centerX="0.5" centerY="0.5" radius="0.5">
               <stops>
                  <Stop color="BLACK" />
                  <Stop color="BLACK" offset="1.0" />
               </stops>
            </RadialGradient>
         </textFill></JFXButton>
      <JFXButton fx:id="ingles" layoutX="359.0" layoutY="58.0" onAction="#cambiarIdiomaIngles" text="%english" underline="true">
         <textFill>
            <RadialGradient centerX="0.5" centerY="0.5" radius="0.5">
               <stops>
                  <Stop color="BLACK" />
                  <Stop color="BLACK" offset="1.0" />
               </stops>
            </RadialGradient>
         </textFill></JFXButton>
   </children>
   <effect>
      <ColorAdjust contrast="0.15" />
