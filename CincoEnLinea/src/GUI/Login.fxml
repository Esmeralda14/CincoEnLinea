<AnchorPane id="AnchorPane" prefHeight="549.0" prefWidth="444.0" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="GUI.LoginController">
   <children>
      <Rectangle arcHeight="5.0" arcWidth="5.0" fill="#45b3b9" height="64.0" layoutX="-14.0" layoutY="-11.0" stroke="#45b3b9" strokeType="INSIDE" width="458.0" />
      <Label fx:id="inicioSesion" layoutX="117.0" layoutY="5.0" text="%inicioSesion">
         <font>
            <Font name="Myanmar Sangam MN Bold" size="36.0" />
         </font>
      </Label>
      <Label fx:id="labelUsuario" layoutX="155.0" layoutY="247.0" text="%labelUsuario">
         <font>
            <Font name="Myanmar Sangam MN" size="19.0" />
         </font>
      </Label>
      <TextField fx:id="fieldUsuario" layoutX="98.0" layoutY="287.0" prefHeight="27.0" prefWidth="250.0">
         <effect>
            <ColorAdjust contrast="-0.29" />
         </effect></TextField>
      <Label fx:id="labelContraseña" layoutX="140.0" layoutY="327.0" text="%labelContrasena">
         <font>
            <Font name="Myanmar Sangam MN" size="19.0" />
         </font>
      </Label>
      <Button fx:id="ingresar" layoutX="156.0" layoutY="421.0" mnemonicParsing="false" onAction="#iniciarSesion" prefHeight="37.0" prefWidth="142.0" text="%ingresar">
